private void Form1_Load(object sender, EventArgs e)
        {
string strketqua = "";
            //khai bao 1 doi tuong mèo
            cat tom = new cat();
            cat jonh = new cat();
            // gán thuộc tính
            tom.Somat = 2;
            tom.Sochan = 4;
            tom.Maulong = "mau đen";
            // hiện thị thông tin của loài mèo
            strketqua += "thông tin của mèo \r\n";
            strketqua += "số mắt" + tom.Somat+ "\r\n";
            strketqua += "số chân" + tom.Sochan+ "\r\n";
            strketqua += "màu lông" + tom.Maulong+ "\r\n";
            strketqua += "tiếng kêu" + tom._tiengkeu()+"\r\n";
            // hiển thị thông tin của loài chó
            strketqua += "thông tin của chó \r\n";
            strketqua += "số mắt" + jonh.Somat + "\r\n";
            strketqua += "số chân" + jonh.Sochan + "\r\n";
            strketqua += "màu lông" + jonh.Maulong + "\r\n";
            strketqua += "tiếng kêu" + jonh._tiengkeu() + "\r\n";

            txtthongtin.Text = strketqua;
            
            //ke thua lop 
